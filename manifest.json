{
  "manifest_version": 2,
  "name": "tabby",
  "version": "1.0",
  "description": "View your tabs and windows from one place.",
  "permissions": [
    "<all_urls>",
    "tabs"
  ],
  "background": {
    "scripts": [
      "dist/background.js"
    ]
  },
  "browser_action": {},
  "commands": {
    "open": {
      "suggested_key": {
        "default": "Ctrl+Shift+Y"
      },
      "description": "Opens a new tab to display all of your tabs"
    }
  }
}